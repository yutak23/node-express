{
	"name": "node-express",
	"description": "My webpack project",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "git@github.com:yuta-katayama-23/node-express.git",
	"author": "yuta-katayama-23",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.118.1",
		"@aws-sdk/lib-dynamodb": "^3.118.1",
		"ajv": "^8.11.0",
		"ajv-formats": "^2.1.1",
		"any-base": "^1.1.0",
		"app-root-path": "^3.0.0",
		"bcrypt": "^5.0.1",
		"camelcase-keys": "^7.0.2",
		"commander": "^9.3.0",
		"compression": "^1.7.4",
		"config": "^3.3.7",
		"express": "^4.18.1",
		"express-openapi-validator": "^4.13.8",
		"lodash": "^4.17.21",
		"luxon": "^2.4.0",
		"make-error": "^1.3.6",
		"mysql2": "^2.3.3",
		"pad-left": "^2.1.0",
		"sequelize": "^6.21.2",
		"snakecase-keys": "^5.4.2",
		"source-map-support": "^0.5.21"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@babel/eslint-parser": "^7.18.2",
		"@babel/plugin-proposal-throw-expressions": "^7.18.6",
		"@babel/preset-env": "^7.18.6",
		"@babel/register": "^7.18.6",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"babel-loader": "^8.2.5",
		"core-js": "^3.23.3",
		"eslint": "^8.18.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^26.5.3",
		"eslint-webpack-plugin": "^3.2.0",
		"jest": "^28.1.2",
		"license-checker": "^25.0.1",
		"lint-staged": "^13.0.3",
		"nodemon": "^2.0.18",
		"prettier": "^2.7.1",
		"regenerator-runtime": "^0.13.9",
		"sequelize-cli": "^6.4.1",
		"simple-git-hooks": "^2.8.0",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"webpack-node-externals": "^3.0.0"
	},
	"scripts": {
		"dev": "webpack watch --node-env=development",
		"build": "webpack --node-env=production",
		"debug": "webpack --node-env=development",
		"start:watch": "nodemon dist/index.js",
		"lint": "eslint src/",
		"test": "jest",
		"mysqldump:full": "docker exec -t mysql mysqldump -u root --databases node-express > ./mysql/sql/full.sql",
		"mysqldump:schema": "docker exec -t mysql mysqldump -u root --databases node-express --no-data | sed -e 's/ AUTO_INCREMENT=[0-9]*//' > ./mysql/sql/1_schema.sql",
		"mysqldump:data": "docker exec -t mysql mysqldump -u root -t --databases node-express --no-create-db > ./mysql/sql/2_data.sql",
		"create:table": "aws dynamodb create-table --cli-input-json file://./commands/create-table.json --endpoint-url http://localhost:8000",
		"prepare": "npx simple-git-hooks"
	},
	"lint-staged": {
		"*": "npx prettier --ignore-unknown --write",
		"yarn.lock": "npx license-checker --production --failOn \"GPL;AGPL;LGPL;NGPL\" --summary"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged --verbose",
		"commit-msg": "npx commitlint -e"
	}
}
